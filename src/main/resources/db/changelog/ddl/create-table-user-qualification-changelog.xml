<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="070720241943" author="Roman">
        <createTable tableName="user_qualification">
            <column name="user_id" type="bigserial">
                <constraints
                        primaryKey="true"
                        primaryKeyName="pk_user_qualification"
                        foreignKeyName="fk_user_qualification_user"
                        references="user(id)"
                />
            </column>
            <column name="qualification_id" type="smallint">
                <constraints
                        primaryKey="true"
                        primaryKeyName="pk_user_qualification"
                        foreignKeyName="fk_user_qualification_qualification"
                        references="qualification(id)"
                />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>